#include<stdio.h>
#include<string.h>
int main(){
	int n;
	char s1[20],s2[20];
	printf("enter the string:");
	scanf("%s",&s1);
	int len=strlen(s1);
	for(int i=len-1;i>=0;i--){
		s2[len-i-1]=s1[i];
	}
	s2[len]='\0';
	if(strcmp(s1,s2)==0){
		printf("it is a palindrome:");
	}
	else{
		printf("it is not a palindrome:");
	}
	return 0;
}
