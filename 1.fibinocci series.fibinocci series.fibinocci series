#include<stdio.h>
    int fibinocci(int n){ 
    	if(n<=1)
    	return n;
    	else
    	   return(fibinocci(n-1)+fibinocci(n-2));
	}
	int main(){
		int n,i;
		printf("enter the number of terms:");
		scanf("%d",&n);
		printf("fibinocci series:");
		for(i=0;i<n;i++){\
			printf(" %d ",fibinocci(i));
		}	
		return 0;
}
